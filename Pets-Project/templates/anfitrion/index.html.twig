{% extends 'base.html.twig' %}

{% block title %}Anfitrion index{% endblock %}

{% block body %}
    <h1>Anfitrion index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Cuenta</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for anfitrion in anfitrions %}
            <tr>
                <td>{{ anfitrion.id }}</td>
                <td>{{ anfitrion.cuenta }}</td>
                <td>
                    <a href="{{ path('anfitrion_show', {'id': anfitrion.id}) }}">show</a>
                    <a href="{{ path('anfitrion_edit', {'id': anfitrion.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('anfitrion_new') }}">Create new</a>
{% endblock %}
